<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Toolbar -->
    <SolidColorBrush x:Key="ToolbarBackground" Color="#231C18" />
    <SolidColorBrush x:Key="ToolbarBorderBrush" Color="#4A3F35" />
    <SolidColorBrush x:Key="ContentBackground" Color="#1E1814" />

    <!-- Tab buttons -->
    <SolidColorBrush x:Key="TabButtonBorderBrush" Color="#5A4D42" />
    <SolidColorBrush x:Key="TabButtonHoverBorderBrush" Color="#9A8575" />
    <SolidColorBrush x:Key="TabButtonForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="TabButtonActiveBackground" Color="#3D2820" />
    <SolidColorBrush x:Key="TabButtonActiveBorderBrush" Color="#D4854A" />
    <SolidColorBrush x:Key="TabButtonInactiveBackground" Color="Transparent" />

    <!-- Add button -->
    <SolidColorBrush x:Key="AddButtonBorderBrush" Color="#5A4D42" />
    <SolidColorBrush x:Key="AddButtonForeground" Color="#9A8575" />

    <!-- Empty state -->
    <SolidColorBrush x:Key="EmptyStateForeground" Color="#9A8575" />

    <!-- Tab icon -->
    <SolidColorBrush x:Key="TabIconNoSessionForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="TabIconInactiveDiamondForeground" Color="#C48BC0" />
    <SolidColorBrush x:Key="TabIconIdleForeground" Color="#8BBF65" />
    <SolidColorBrush x:Key="TabIconWorkingForeground" Color="#D4854A" />
    <SolidColorBrush x:Key="TabIconWaitingForeground" Color="#E6A96F" />
    <SolidColorBrush x:Key="TabIconErrorForeground" Color="#E06B5A" />

    <!-- File preview -->
    <SolidColorBrush x:Key="PreviewEmptyForeground" Color="#9A8575" />
    <SolidColorBrush x:Key="PreviewNoPreviewForeground" Color="#9A8575" />
    <SolidColorBrush x:Key="PreviewBackground" Color="#1A1412" />
    <SolidColorBrush x:Key="PreviewForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="PreviewLinkForeground" Color="#E6A96F" />
    <SolidColorBrush x:Key="PreviewLineNumberForeground" Color="#6A5A4A" />

    <!-- Git status -->
    <SolidColorBrush x:Key="GitPlaceholderForeground" Color="#9A8575" />
    <SolidColorBrush x:Key="GitStagedForeground" Color="#8BBF65" />
    <SolidColorBrush x:Key="GitUnstagedForeground" Color="#E6A96F" />
    <SolidColorBrush x:Key="GitStagedLabelForeground" Color="#9A8575" />

    <!-- File explorer -->
    <SolidColorBrush x:Key="ExplorerToolbarBackground" Color="#231C18" />
    <SolidColorBrush x:Key="ExplorerToolbarBorderBrush" Color="#4A3F35" />
    <SolidColorBrush x:Key="ExplorerForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="ExplorerSelectedBackground" Color="#3D2820" />
    <SolidColorBrush x:Key="ExplorerSelectedForeground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="ExplorerHoverBackground" Color="#2D241E" />

    <!-- Panel placeholder -->
    <SolidColorBrush x:Key="PlaceholderBackground" Color="#1E1814" />
    <SolidColorBrush x:Key="PlaceholderTitleForeground" Color="#9A8575" />
    <SolidColorBrush x:Key="PlaceholderSubtitleForeground" Color="#7A6A5A" />

    <!-- Title bar -->
    <SolidColorBrush x:Key="TitleBarBackground" Color="#231C18" />
    <SolidColorBrush x:Key="TitleBarForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="TitleBarButtonHover" Color="#3D2820" />
    <SolidColorBrush x:Key="CloseButtonHover" Color="#C4623A" />
    <SolidColorBrush x:Key="CloseButtonHoverForeground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="MenuIconForeground" Color="#9A8575" />

    <!-- Menu -->
    <SolidColorBrush x:Key="MenuBackground" Color="#231C18" />
    <SolidColorBrush x:Key="MenuForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="MenuSeparatorBrush" Color="#4A3F35" />
    <SolidColorBrush x:Key="MenuHighlightBackground" Color="#3D2820" />
    <SolidColorBrush x:Key="MenuDisabledForeground" Color="#7A6A5A" />
    <SolidColorBrush x:Key="MenuPopupBackground" Color="#1A1412" />
    <SolidColorBrush x:Key="MenuPopupBorderBrush" Color="#4A3F35" />

    <!-- ScrollBar -->
    <SolidColorBrush x:Key="ScrollBarBackground" Color="#2D241E" />
    <SolidColorBrush x:Key="ScrollBarThumbBackground" Color="#5A4D42" />
    <SolidColorBrush x:Key="ScrollBarThumbHover" Color="#9A8575" />
    <SolidColorBrush x:Key="ScrollBarThumbPressed" Color="#E8D5C4" />
    <SolidColorBrush x:Key="ScrollBarArrowForeground" Color="#9A8575" />
    <SolidColorBrush x:Key="ScrollBarArrowHover" Color="#D4854A" />

    <!-- Help / About windows -->
    <SolidColorBrush x:Key="HelpBackground" Color="#1A1412" />
    <SolidColorBrush x:Key="HelpForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="HelpHeadingForeground" Color="#D4854A" />
    <SolidColorBrush x:Key="HelpMutedForeground" Color="#9A8575" />
    <SolidColorBrush x:Key="HelpLinkForeground" Color="#E6A96F" />
    <SolidColorBrush x:Key="HelpCardBackground" Color="#231C18" />
    <SolidColorBrush x:Key="HelpCardBorderBrush" Color="#4A3F35" />
    <SolidColorBrush x:Key="HelpKbdBackground" Color="#2D241E" />
    <SolidColorBrush x:Key="HelpKbdBorderBrush" Color="#5A4D42" />

    <!-- Diff colors -->
    <Color x:Key="DiffAddedColor">#1E2B18</Color>
    <Color x:Key="DiffRemovedColor">#2E1A18</Color>
    <Color x:Key="DiffHunkHeaderColor">#2A1E28</Color>
    <SolidColorBrush x:Key="DiffAddedForeground" Color="#8BBF65" />
    <SolidColorBrush x:Key="DiffRemovedForeground" Color="#E06B5A" />
    <SolidColorBrush x:Key="DiffHunkHeaderForeground" Color="#C48BC0" />
    <SolidColorBrush x:Key="DiffAddedBackground" Color="{StaticResource DiffAddedColor}" />
    <SolidColorBrush x:Key="DiffRemovedBackground" Color="{StaticResource DiffRemovedColor}" />
    <SolidColorBrush x:Key="DiffHunkHeaderBackground" Color="{StaticResource DiffHunkHeaderColor}" />

    <!-- AI Chat panel -->
    <SolidColorBrush x:Key="ChatBackground" Color="#1A1412" />
    <SolidColorBrush x:Key="ChatTextForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="ChatMutedForeground" Color="#9A8575" />
    <SolidColorBrush x:Key="ChatSubtleForeground" Color="#7A6A5A" />

    <!-- Chat status bar -->
    <SolidColorBrush x:Key="ChatStatusBarBackground" Color="#1E1814" />
    <SolidColorBrush x:Key="ChatStatusWorkingForeground" Color="#D4854A" />
    <SolidColorBrush x:Key="ChatStatusWarningForeground" Color="#E6A96F" />
    <SolidColorBrush x:Key="ChatStatusErrorForeground" Color="#E06B5A" />

    <!-- Chat input -->
    <SolidColorBrush x:Key="ChatInputBackground" Color="#1A1412" />
    <SolidColorBrush x:Key="ChatInputForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="ChatInputBorderBrush" Color="#4A3F35" />
    <SolidColorBrush x:Key="ChatInputPromptForeground" Color="#D4854A" />

    <!-- Chat buttons -->
    <SolidColorBrush x:Key="ChatButtonBackground" Color="#A06030" />
    <SolidColorBrush x:Key="ChatButtonForeground" Color="#E8D5C4" />
    <SolidColorBrush x:Key="ChatButtonBorderBrush" Color="#C47840" />

    <!-- Chat agent selection cards -->
    <SolidColorBrush x:Key="ChatAgentCardBackground" Color="#1E1814" />
    <SolidColorBrush x:Key="ChatAgentCardBorderBrush" Color="#4A3F35" />

    <!-- Chat user messages -->
    <SolidColorBrush x:Key="ChatUserBubbleBackground" Color="#3D2820" />
    <SolidColorBrush x:Key="ChatUserLabelForeground" Color="#D4854A" />

    <!-- Chat assistant messages -->
    <SolidColorBrush x:Key="ChatAssistantBubbleBackground" Color="#2D2520" />
    <SolidColorBrush x:Key="ChatAssistantLabelForeground" Color="#8BBF65" />

    <!-- Chat thinking -->
    <SolidColorBrush x:Key="ChatThinkingBackground" Color="#231C18" />
    <SolidColorBrush x:Key="ChatThinkingBorderBrush" Color="#4A3F35" />

    <!-- Chat execution -->
    <SolidColorBrush x:Key="ChatExecutionBackground" Color="#1E2818" />
    <SolidColorBrush x:Key="ChatExecutionBorderBrush" Color="#2A3A20" />
    <SolidColorBrush x:Key="ChatExecutionForeground" Color="#8BBF65" />

    <!-- Chat permission prompt -->
    <SolidColorBrush x:Key="ChatPermissionBackground" Color="#2E2510" />
    <SolidColorBrush x:Key="ChatPermissionBorderBrush" Color="#D4854A" />
    <SolidColorBrush x:Key="ChatPermissionTitleForeground" Color="#E6A96F" />

    <!-- Chat question prompt -->
    <SolidColorBrush x:Key="ChatQuestionBackground" Color="#1E1E28" />
    <SolidColorBrush x:Key="ChatQuestionBorderBrush" Color="#8A7050" />
    <SolidColorBrush x:Key="ChatQuestionTitleForeground" Color="#D4854A" />
    <SolidColorBrush x:Key="ChatOptionButtonBackground" Color="#2D241E" />
    <SolidColorBrush x:Key="ChatOptionButtonBorderBrush" Color="#5A4D42" />

    <!-- Chat warning -->
    <SolidColorBrush x:Key="ChatWarningBackground" Color="#2E2510" />
    <SolidColorBrush x:Key="ChatWarningBorderBrush" Color="#6B4B20" />

    <!-- Chat danger / stop -->
    <SolidColorBrush x:Key="ChatDangerBackground" Color="#2E1A15" />
    <SolidColorBrush x:Key="ChatDangerBorderBrush" Color="#C4623A" />
    <SolidColorBrush x:Key="ChatDangerForeground" Color="#E06B5A" />
    <SolidColorBrush x:Key="ChatDangerMutedForeground" Color="#A06050" />
    <SolidColorBrush x:Key="ChatStopButtonBorderBrush" Color="#C4623A" />
    <SolidColorBrush x:Key="ChatKillButtonBackground" Color="#5A2020" />
    <SolidColorBrush x:Key="ChatKillButtonBorderBrush" Color="#8B3030" />

</ResourceDictionary>
