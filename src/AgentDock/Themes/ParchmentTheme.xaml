<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Toolbar -->
    <SolidColorBrush x:Key="ToolbarBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="ToolbarBorderBrush" Color="#D0C4B0" />
    <SolidColorBrush x:Key="ContentBackground" Color="#FAF5EE" />

    <!-- Tab buttons -->
    <SolidColorBrush x:Key="TabButtonBorderBrush" Color="#C0B49A" />
    <SolidColorBrush x:Key="TabButtonHoverBorderBrush" Color="#8B7D6B" />
    <SolidColorBrush x:Key="TabButtonForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="TabButtonActiveBackground" Color="#E8DCC0" />
    <SolidColorBrush x:Key="TabButtonActiveBorderBrush" Color="#B89030" />
    <SolidColorBrush x:Key="TabButtonInactiveBackground" Color="Transparent" />

    <!-- Add button -->
    <SolidColorBrush x:Key="AddButtonBorderBrush" Color="#C0B49A" />
    <SolidColorBrush x:Key="AddButtonForeground" Color="#8B7D6B" />

    <!-- Empty state -->
    <SolidColorBrush x:Key="EmptyStateForeground" Color="#A09080" />

    <!-- Tab icon -->
    <SolidColorBrush x:Key="TabIconNoSessionForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="TabIconInactiveDiamondForeground" Color="#9070A0" />
    <SolidColorBrush x:Key="TabIconIdleForeground" Color="#3A7A30" />
    <SolidColorBrush x:Key="TabIconWorkingForeground" Color="#8B6914" />
    <SolidColorBrush x:Key="TabIconWaitingForeground" Color="#B88020" />
    <SolidColorBrush x:Key="TabIconErrorForeground" Color="#C44030" />

    <!-- File preview -->
    <SolidColorBrush x:Key="PreviewEmptyForeground" Color="#A09080" />
    <SolidColorBrush x:Key="PreviewNoPreviewForeground" Color="#C0B49A" />
    <SolidColorBrush x:Key="PreviewBackground" Color="#FAF5EE" />
    <SolidColorBrush x:Key="PreviewForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="PreviewLinkForeground" Color="#8B6914" />
    <SolidColorBrush x:Key="PreviewLineNumberForeground" Color="#A09080" />

    <!-- Git status -->
    <SolidColorBrush x:Key="GitPlaceholderForeground" Color="#A09080" />
    <SolidColorBrush x:Key="GitStagedForeground" Color="#3A7A30" />
    <SolidColorBrush x:Key="GitUnstagedForeground" Color="#B88020" />
    <SolidColorBrush x:Key="GitStagedLabelForeground" Color="#A09080" />

    <!-- File explorer -->
    <SolidColorBrush x:Key="ExplorerToolbarBackground" Color="#E8DFD0" />
    <SolidColorBrush x:Key="ExplorerToolbarBorderBrush" Color="#D0C4B0" />
    <SolidColorBrush x:Key="ExplorerForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="ExplorerSelectedBackground" Color="#E8DCC0" />
    <SolidColorBrush x:Key="ExplorerSelectedForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="ExplorerHoverBackground" Color="#F0E8DC" />

    <!-- Panel placeholder -->
    <SolidColorBrush x:Key="PlaceholderBackground" Color="#FAF5EE" />
    <SolidColorBrush x:Key="PlaceholderTitleForeground" Color="#8B7D6B" />
    <SolidColorBrush x:Key="PlaceholderSubtitleForeground" Color="#A09080" />

    <!-- Title bar -->
    <SolidColorBrush x:Key="TitleBarBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="TitleBarForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="TitleBarButtonHover" Color="#E8DFD0" />
    <SolidColorBrush x:Key="CloseButtonHover" Color="#C44030" />
    <SolidColorBrush x:Key="CloseButtonHoverForeground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="MenuIconForeground" Color="#8B7D6B" />

    <!-- Menu -->
    <SolidColorBrush x:Key="MenuBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="MenuForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="MenuSeparatorBrush" Color="#D0C4B0" />
    <SolidColorBrush x:Key="MenuHighlightBackground" Color="#E8DCC0" />
    <SolidColorBrush x:Key="MenuDisabledForeground" Color="#C0B49A" />
    <SolidColorBrush x:Key="MenuPopupBackground" Color="#FAF5EE" />
    <SolidColorBrush x:Key="MenuPopupBorderBrush" Color="#D0C4B0" />

    <!-- Icon button hover (subtle, for small toolbar/icon buttons) -->
    <SolidColorBrush x:Key="IconButtonHoverBackground" Color="#E8DFD0" />

    <!-- ScrollBar -->
    <SolidColorBrush x:Key="ScrollBarBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="ScrollBarThumbBackground" Color="#D0C4B0" />
    <SolidColorBrush x:Key="ScrollBarThumbHover" Color="#B8AA96" />
    <SolidColorBrush x:Key="ScrollBarThumbPressed" Color="#8B7D6B" />
    <SolidColorBrush x:Key="ScrollBarArrowForeground" Color="#A09080" />
    <SolidColorBrush x:Key="ScrollBarArrowHover" Color="#B89030" />

    <!-- Help / About windows -->
    <SolidColorBrush x:Key="HelpBackground" Color="#FAF5EE" />
    <SolidColorBrush x:Key="HelpForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="HelpHeadingForeground" Color="#8B6914" />
    <SolidColorBrush x:Key="HelpMutedForeground" Color="#A09080" />
    <SolidColorBrush x:Key="HelpLinkForeground" Color="#8B6914" />
    <SolidColorBrush x:Key="HelpCardBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="HelpCardBorderBrush" Color="#D0C4B0" />
    <SolidColorBrush x:Key="HelpKbdBackground" Color="#E8DFD0" />
    <SolidColorBrush x:Key="HelpKbdBorderBrush" Color="#D0C4B0" />

    <!-- Diff colors -->
    <Color x:Key="DiffAddedColor">#E8F5E0</Color>
    <Color x:Key="DiffRemovedColor">#FAE0D8</Color>
    <Color x:Key="DiffHunkHeaderColor">#EDE0F0</Color>
    <SolidColorBrush x:Key="DiffAddedForeground" Color="#2D7A20" />
    <SolidColorBrush x:Key="DiffRemovedForeground" Color="#B82020" />
    <SolidColorBrush x:Key="DiffHunkHeaderForeground" Color="#7030A0" />
    <SolidColorBrush x:Key="DiffAddedBackground" Color="{StaticResource DiffAddedColor}" />
    <SolidColorBrush x:Key="DiffRemovedBackground" Color="{StaticResource DiffRemovedColor}" />
    <SolidColorBrush x:Key="DiffHunkHeaderBackground" Color="{StaticResource DiffHunkHeaderColor}" />

    <!-- AI Chat panel -->
    <SolidColorBrush x:Key="ChatBackground" Color="#FAF5EE" />
    <SolidColorBrush x:Key="ChatTextForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="ChatMutedForeground" Color="#8B7D6B" />
    <SolidColorBrush x:Key="ChatSubtleForeground" Color="#A09080" />

    <!-- Chat status bar -->
    <SolidColorBrush x:Key="ChatStatusBarBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="ChatStatusWorkingForeground" Color="#8B6914" />
    <SolidColorBrush x:Key="ChatStatusWarningForeground" Color="#B88020" />
    <SolidColorBrush x:Key="ChatStatusErrorForeground" Color="#C44030" />

    <!-- Chat input -->
    <SolidColorBrush x:Key="ChatInputBackground" Color="#FAF5EE" />
    <SolidColorBrush x:Key="ChatInputForeground" Color="#3B3020" />
    <SolidColorBrush x:Key="ChatInputBorderBrush" Color="#D0C4B0" />
    <SolidColorBrush x:Key="ChatInputPromptForeground" Color="#8B6914" />

    <!-- Chat buttons -->
    <SolidColorBrush x:Key="ChatButtonBackground" Color="#A07810" />
    <SolidColorBrush x:Key="ChatButtonForeground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="ChatButtonBorderBrush" Color="#B89030" />

    <!-- Chat agent selection cards -->
    <SolidColorBrush x:Key="ChatAgentCardBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="ChatAgentCardBorderBrush" Color="#D0C4B0" />

    <!-- Chat user messages -->
    <SolidColorBrush x:Key="ChatUserBubbleBackground" Color="#E8DCC0" />
    <SolidColorBrush x:Key="ChatUserLabelForeground" Color="#6B4C11" />

    <!-- Chat assistant messages -->
    <SolidColorBrush x:Key="ChatAssistantBubbleBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="ChatAssistantLabelForeground" Color="#3A7A30" />

    <!-- Chat thinking -->
    <SolidColorBrush x:Key="ChatThinkingBackground" Color="#F0E8DC" />
    <SolidColorBrush x:Key="ChatThinkingBorderBrush" Color="#D0C4B0" />

    <!-- Chat execution -->
    <SolidColorBrush x:Key="ChatExecutionBackground" Color="#F0F5E8" />
    <SolidColorBrush x:Key="ChatExecutionBorderBrush" Color="#C0D8B0" />
    <SolidColorBrush x:Key="ChatExecutionForeground" Color="#3A7A30" />

    <!-- Chat permission prompt -->
    <SolidColorBrush x:Key="ChatPermissionBackground" Color="#FFF0D0" />
    <SolidColorBrush x:Key="ChatPermissionBorderBrush" Color="#D09020" />
    <SolidColorBrush x:Key="ChatPermissionTitleForeground" Color="#B88020" />

    <!-- Chat question prompt -->
    <SolidColorBrush x:Key="ChatQuestionBackground" Color="#E8E0F0" />
    <SolidColorBrush x:Key="ChatQuestionBorderBrush" Color="#8070A0" />
    <SolidColorBrush x:Key="ChatQuestionTitleForeground" Color="#5A4080" />
    <SolidColorBrush x:Key="ChatOptionButtonBackground" Color="#E8E0F0" />
    <SolidColorBrush x:Key="ChatOptionButtonBorderBrush" Color="#B0A0C8" />

    <!-- Chat warning -->
    <SolidColorBrush x:Key="ChatWarningBackground" Color="#FFF0D0" />
    <SolidColorBrush x:Key="ChatWarningBorderBrush" Color="#D09020" />

    <!-- Chat danger / stop -->
    <SolidColorBrush x:Key="ChatDangerBackground" Color="#FDE8E0" />
    <SolidColorBrush x:Key="ChatDangerBorderBrush" Color="#C44030" />
    <SolidColorBrush x:Key="ChatDangerForeground" Color="#B82020" />
    <SolidColorBrush x:Key="ChatDangerMutedForeground" Color="#D08060" />
    <SolidColorBrush x:Key="ChatStopButtonBorderBrush" Color="#C44030" />
    <SolidColorBrush x:Key="ChatKillButtonBackground" Color="#FAE0D8" />
    <SolidColorBrush x:Key="ChatKillButtonBorderBrush" Color="#C44030" />

</ResourceDictionary>
